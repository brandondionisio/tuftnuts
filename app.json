{
  "expo": {
    "name": "TuftNuts",
    "slug": "TuftNuts",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/squirrel-svgrepo-com.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/transparent.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCkDiAe2NAVMhrKJxs1S3f0uQ91HVQ5Ico"
        }
      },
      "supportsTablet": true,
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "We need access to your photo library to upload images.",
        "NSPhotoLibraryAddUsageDescription": "We need access to save images to your library."
      },
      "bundleIdentifier": "com.anonymous.TuftNuts"
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCkDiAe2NAVMhrKJxs1S3f0uQ91HVQ5Ico"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.TuftNuts"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "ebc159a7-ffd0-48c6-a24e-b112b6c508e3"
      }
    }
  }
}
